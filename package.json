{
  "name": "ts-gql",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "prisma": "prisma",
    "docs": "tidil docs --tpl ts-gql",
    "setup": "tidil setup --tpl ts-gql",
    "build": "npm run build:reset && NODE_ENV=production $(npm bin)/tsc && npm run build:schema",
    "prisma:build": "docker-compose build --no-cache prisma",
    "prisma:run": "docker-compose run prisma",
    "generate": "npm run prisma:generate && graphqlgen",
    "prisma:generate": "prisma generate",
    "build:schema": "mkdir -p dist && npm run prisma:generate && NODE_ENV=production ts-node $(npm root)/@etidbury/ts-gql-helpers/util/build-schema.js",
    "build:reset": "rm -rf dist",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "NODE_ENV=production $(npm bin)/jest",
    "test:watch": "$(npm bin)/jest --watch --runInBand",
    "test:ci": "npm run eslint && touch out.log ; DEBUG=true npm run dev > out.log & (tidil check-server --port-env PORT --wait 20 && npm run test) || (tail -n 1000 out.log ; rm out.log ; exit 1)",
    "dev": "NODE_ENV=development ts-node src/index.ts",
    "watch": "$(npm bin)/nodemon --config $(npm root)/@etidbury/ts-gql-helpers/config/nodemon.json --exec npm run dev",
    "prod": "npm start",
    "docker-compose": "docker-compose -f $(npm root)/@etidbury/ts-gql-helpers/config/docker-compose.yml",
    "prisma:dev": "npm run docker-compose up -d",
    "docker": "npm run docker:build && npm run docker:run",
    "docker:build": "npm run build && docker build -t \"$(echo $npm_package_name)\" -f $(npm root)/@etidbury/ts-gql-helpers/config/Dockerfile .",
    "docker:run": "docker run -p 4001:80 $(echo $npm_package_name):latest",
    "mysql:docker": "docker stop local-mysql-server ; docker rm local-mysql-server ; docker run --name local-mysql-server -p 3306:3306 -e MYSQL_ALLOW_EMPTY_PASSWORD=true -e MYSQL_DATABASE=db_$(echo $npm_package_name) -e MYSQL_ROOT_HOST=% -d mysql/mysql-server:5.7 --character-set-server=utf8mb4 --collation-server=utf8mb4_unicode_ci",
    "eslint": "$(npm bin)/eslint --fix --max-warnings 300 ./"
  },
  "dependencies": {
    "@etidbury/ts-gql-helpers": "^0.0.7",
    "apollo-server": "^2.1.0",
    "apollo-server-core": "^2.1.0",
    "axios": "0.18.0",
    "dotenv": "^6.1.0",
    "graphql-import": "^0.7.1",
    "http-errors": "^1.7.1",
    "prisma": "1.19.0",
    "prisma-binding": "^2.1.6",
    "prisma-client-lib": "1.19.0",
    "ts-node": "^7.0.1",
    "url-join": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@types/chai": "^4.1.7",
    "@types/es6-promise": "^3.3.0",
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "chai": "^4.2.0",
    "display-notification": "^2.0.0",
    "eslint": "5.3.0",
    "eslint-cli": "^1.1.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-typescript": "^0.12.0",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.2",
    "graphqlgen": "^0.3.0",
    "jest": "22.4.3",
    "json-to-ts": "^1.5.4",
    "nodemon": "^1.18.4",
    "osascript": "^1.2.0",
    "regenerator-runtime": "^0.12.1",
    "ts-jest": "^23.10.4",
    "tsc": "^1.20150623.0",
    "typescript": "^3.1.3"
  },
  "resolutions": {
    "graphql": "^14.0.2"
  }
}
